{
    "env": {
        "browser": true,
        "jquery": true,
        "node": true
    },
    "globals": {},
    "plugins": [],
    "extends": ["eslint:recommended", "angular"],
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}